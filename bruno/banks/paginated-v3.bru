meta {
  name: paginated-v3
  type: http
  seq: 9
}

post {
  url: {{urlBase}}api/v1/banks/paginated
  body: json
  auth: bearer
}

auth:bearer {
  token: {{access_token}}
}

body:json {
  {
      "sortBy": ["id"],
      "sortDesc": [true],
      "filterBy": ["name"],
      "filterValues": ["%banco%"],
      "rowsPerPage": 15,
      "page": 1
  }
}
